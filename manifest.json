{
	"manifest_version": 2,

	"name": "Shoes Gonger",
	"description": "Created by ZahNarztiK",
	"version": "1.0",
	"icons": {
		"16":"icon/icon16.png",
		"32":"icon/icon32.png",
		"64":"icon/icon64.png",
		"128":"icon/icon128.png",
		"256":"icon/icon256.png",
		"512":"icon/icon512.png"
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["jquery-1.7.2.js"]
		}
	],

	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},

	"browser_action": {
		"default_icon": "icon/icon16.png",
		"default_popup": "popup.html"
	},

	"permissions": [
		"activeTab", "browsingData", "tabs", "webNavigation", "*://*/*"
	]
}
